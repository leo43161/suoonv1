{
  "expo": {
    "name": "suoon-app",
    "slug": "suoon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suoon.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.suoon.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cd435f60-02cc-423f-8e73-0add8c10a133"
      }
    },
    "plugins": [
      "expo-location",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Sora/Sora.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Sora",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Sora/Sora-ExtraBold.ttf",
                    "weight": 800
                  },
                  {
                    "path": "./assets/fonts/Sora/Sora-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Sora/Sora-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Sora/Sora-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/Sora/Sora-Light.ttf",
                    "weight": 300
                  }
                ]
              }
            ]
          }
        }
      ]
    ],
    "owner": "leo43"
  }
}